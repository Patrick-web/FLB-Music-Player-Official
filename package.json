{
	"name": "flbmusic",
	"productName": "FLB Music",
	"appId": "com.flb.flbmusic",
	"author": {
		"name": "Patrick Waweru",
		"email": "pntxall100@gmail.com"
	},
	"version": "1.1.7",
	"description": "A Beautiful and Feature Rich Music Player",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Patrick-web/FLB-Music-Player-Official.git"
	},
	"scripts": {
		"start": "vue-cli-service electron:serve",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"test:unit": "vue-cli-service test:unit",
		"lint": "vue-cli-service lint",
		"e:build": "vue-cli-service electron:build",
		"push-update": "vue-cli-service electron:build -- -p always",
		"postinstall": "electron-builder install-app-deps",
		"postuninstall": "electron-builder install-app-deps",
		"deploy:steps": "git commit -am vx.x.x && git tag vx.x.x && git push --tags"
	},
	"main": "background.js",
