		"node-id3": "0.2.3",
		"vue": "2.6.14",
		"vue-router": "3.5.2",
		"vue-virtual-scroll-list": "2.3.2",
		"vue-virtual-scroller": "1.0.10",
		"vuedraggable": "2.24.3",
		"vuex": "3.6.2",
		"ytdl-core": "4.8.3"
	},
	"devDependencies": {
