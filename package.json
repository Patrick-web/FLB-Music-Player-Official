		"node-downloader-helper": "1.0.18",
		"node-id3": "0.2.3",
		"vue": "2.6.14",
		"vue-router": "3.5.2",
		"vue-virtual-scroll-list": "2.3.2",
		"vue-virtual-scroller": "1.0.10",
		"vuedraggable": "2.24.3",
		"vuex": "3.6.2",
		"ytdl-core": "4.9.1"
	},
	"devDependencies": {
		"@types/electron-devtools-installer": "2.2.0",
		"@typescript-eslint/eslint-plugin": "4.33.0",
